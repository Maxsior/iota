{
  "plugins": ["import"],
  "extends": ["airbnb-typescript/base"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "arrow-parens": ["error", "as-needed"],
    "no-console": "off",
    "class-methods-use-this": "warn"
  }
}
